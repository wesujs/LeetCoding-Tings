class Solution:
    def twoSum(self, numbers: List[int], target: int) -> List[int]:

        lp = 0
        rp = len(numbers) -1

        while lp <= rp:
            sumx = numbers[lp] + numbers[rp]
            if sumx == target:
                newl = lp + 1
                newr = rp + 1
                return [newl, newr]
            elif sumx > target:
                rp -= 1
            else:
                lp += 1
        return []


"""
Runtime: 129 ms
Beats: 89.93%

Memory: 17.2 MB
Beats: 94.34%

"""
